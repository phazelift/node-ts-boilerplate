{
   "name": "node-ts-boilerplate",
   "version": "1.1.0",
   "description": "nodejs typescript eslint prettier (es-)jest boilerplate",
   "main": "./build/index.js",
   "scripts": {
      "start": "node ./build/index.js",
      "lint": "eslint '**/*.ts' --fix --cache",
      "build": "npm run lint && tsc",
      "test": "npm run build && jest"
   },
   "devDependencies": {
      "@types/jest": "^25.2.1",
      "@types/node": "^13.11.1",
      "@typescript-eslint/eslint-plugin": "^2.28.0",
      "@typescript-eslint/parser": "^2.28.0",
      "eslint": "^6.8.0",
      "eslint-config-prettier": "^6.10.1",
      "eslint-plugin-jest": "^23.8.2",
      "eslint-plugin-prettier": "^3.1.3",
      "jest": "^28.1.0",
      "prettier": "2.0.4",
      "ts-jest": "^25.3.1",
      "typescript": "^3.8.3"
   },
   "keywords": [
      "boilerplate",
      "nodejs",
      "typescript",
      "ts"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/phazelift/node-ts-boilerplate"
   },
   "author": "dennis raymondo van der sluis",
   "bugs": {
      "url": "https://github.com/phazelift/node-ts-boilerplate/issues"
   },
   "homepage": "https://github.com/phazelift/node-ts-boilerplate",
